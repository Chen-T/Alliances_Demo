</view>
<!--定义模板-->
<template name="mine_item">
  <view class="mine_flex">
    <image src='{{item_images}}' class='item_image' />
    <view class="mine_descMark">
      <text class='mine_description'>{{mine_description}}</text>
      <image src="{{mine_go}}" class="mine_mark"/>
    </view>
  </view>
</template>
<template is="mine_item" data="{{...personal_information}}" />
